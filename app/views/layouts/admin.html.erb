<!DOCTYPE html>
<html>
  <head>
    <title>Admin - Duke Girls</title>
    <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'admin/application', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag "https://unpkg.com/masonry-layout@4.0/dist/masonry.pkgd.min.js" %>
    <%= javascript_include_tag "https://unpkg.com/imagesloaded@4.1/imagesloaded.pkgd.min.js" %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'admin/layouts/admin_nav' %>
    <%= render 'admin/layouts/admin_sidemenu' %>  
    <div class="admin_main">
      <%= render 'shared/alert' %>
      <%= yield %>
    </div>
  </body>
  <footer></footer>
</html>
