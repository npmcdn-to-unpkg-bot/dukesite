<div class="page-header">
  <h1>Quotes <span class="badge"><%= @quotes.size %></span></h1>
</div>

<p>
  <a data-toggle="collapse" href="#collapseAddQuoteForm" aria-expanded="false">
    <span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span>
     Add Quote
  </a>
</p>
<%= render "shared/error", object: @quote %>
<div class="collapse in" id="collapseAddQuoteForm">
  <div class="row">
    <div class="col-xs-12 col-sm-8 well">
      <%= render "form" %>
    </div>
  </div>
</div>

<% @quotes.each do |q| %>
  <%= q.title %>
  <%= q.description %>
  <% q.photos.each do |p| %>
    <%= p.image.url %>
  <% end %>
<% end %>
